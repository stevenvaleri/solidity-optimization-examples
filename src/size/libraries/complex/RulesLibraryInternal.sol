        require(number > 11, "This is an invalid number 11");
        require(number > 12, "This is an invalid number 12");
        require(number > 13, "This is an invalid number 13");
        require(number > 14, "This is an invalid number 14");
        require(number > 15, "This is an invalid number 15");
        require(number > 16, "This is an invalid number 16");
        require(number > 17, "This is an invalid number 17");
        require(number > 18, "This is an invalid number 18");
        require(number > 19, "This is an invalid number 19");
        require(number > 20, "This is an invalid number 20");
        require(number > 21, "This is an invalid number 21");
